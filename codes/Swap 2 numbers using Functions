#include <stdio.h>

// Function to get the swapped value of the first integer
int getSwappedFirst(int a, int b) {
    return b;
}

// Function to get the swapped value of the second integer
int getSwappedSecond(int a, int b) {
    return a;
}

int main() {
    int num1, num2;
    int swapped1, swapped2;

    // Get input from the user
    printf("Enter the first integer: ");
    scanf("%d", &num1);
    printf("Enter the second integer: ");
    scanf("%d", &num2);

    printf("\nBefore swapping:\n");
    printf("First integer: %d\n", num1);
    printf("Second integer: %d\n", num2);

    // Use functions to swap values without pointers
    swapped1 = getSwappedFirst(num1, num2);
    swapped2 = getSwappedSecond(num1, num2);

    printf("\nAfter swapping:\n");
    printf("First integer: %d\n", swapped1);
    printf("Second integer: %d\n", swapped2);

    return 0;
}
