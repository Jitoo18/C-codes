#include <stdio.h>

int main()
{
    int n = 9; // Size of the array
    int a[n];  // Declare the array
    int sum = 0;

    // Prompt user for input
    printf("Enter %d numbers:\n", n);
    for (int i = 0; i < n; i++)
    {
        printf("Enter number %d: ", i + 1);
        scanf("%d", &a[i]); // Read user input into the array
    }

    // Calculate the sum
    for (int i = 0; i < n; i++)
    {
        sum += a[i]; // Add each element to sum
    }

    // Print the sum
    printf("Sum is %d\n", sum);

    return 0; // Correct return statement
}

